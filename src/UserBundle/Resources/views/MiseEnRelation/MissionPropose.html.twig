    <style>
        .hidden{display: none !important;}
        .ui-widget-header{color:black;}



        .col-md-2, .col-md-10{
            padding:0;
        }
        .panel{
            margin-bottom: 0px;
            background-color: #66afe9;
        }
        .chat-window{
            bottom:0;


            margin-left:10px;
        }
        .chat-window > div > .panel{
