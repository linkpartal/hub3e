        }

        .messages > p {
            font-size: 13px;
            margin: 0 0 0.2rem 0;
        }
        .messages > time {
            font-size: 11px;
            color: grey;
        }
        .msg_container {
            padding: 10px;

            display: flex;
        }
        img {
            display: block;
            width: 100%;
        }
