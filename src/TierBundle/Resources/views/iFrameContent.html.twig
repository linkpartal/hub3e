        function checkFileSize () {
            var approuved = true;
            var notApprouved = "";

            // call them as such files[0].size will get you the file size of the 0th file
            $("#document_apprenant").find("input[type='file']").each(function(){
                var filesize = ((this.files[0].size/1024)/1024).toFixed(4); // MB
                if (this.files[0].name != "item" && typeof this.files[0].name != "undefined" && filesize >= 1) {
                    approuved= false;
                    notApprouved += ' '+this.files[0].name;
                }
            });
            document.getElementById('submitaddapprenant').disabled = approuved;
            if(approuved){
                dupliquerDocument();
                $("#document_apprenant").find('button').on("click",checkFileSize );
            }
            else{
                alert(notApprouved +"sont trop grand");
                $("#document_apprenant").find("input[type='file']").last().val('');
            }
        }

