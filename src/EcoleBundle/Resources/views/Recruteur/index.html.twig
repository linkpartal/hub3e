    <div onclick="document.getElementById('MissionPourvues').click();hide(this)" class="close"></div>
    <div class="col-xs-offset-2 col-xs-8 formContainer">
        <span class="glyphicon glyphicon-remove" aria-hidden="true" style="float:right; height:40px; font-size:20px"onclick="this.parentElement.previousElementSibling.click();"> </span>
            <div> <p> <h3> MISSIONS POURVUES </h3> </p></div>
            <table class="table table-bordered table-hover table-responsive">
                <thead style="background-color:#3988d7">
                    <tr>
                        <th> INTITULE </th>
                        <th> SOCIETE </th>
                        <th> DATE DEPOT </th>
                        <th> ACTION </th>
                    </tr>
                </thead>
                <tbody>
                    {% if missions is defined %}
                        {% for m in missions %}
                            {% if m.getPourvue() == 1 %}
                                <tr>
                                    <td> {{ m.intitule }} </td>
                                    <td> {{ m.etablissement.tier.raisonsoc }} </td>
                                    <td> {{ m.datecreation | date ('m/Y') }} </td>
                                    <td> 
                                        <button class= "btn btn-default btn-xs"> 
                                            <a href="{{ path('admin_afficheMission',{'id': m.getId,'idFor': "-1" } ) }}" target="_blank">
                                                <span class= "glyphicon glyphicon-search" aria-hidden="true"></span> 
                                            </a>
                                        </button>
                                    </td>
                                </tr>
                            {% elseif missions == null %}
                                <tr> Aucunes Missions à afficher </tr>
                            {% endif %}
                        {% endfor %}
                    {% endif %}
                </tbody>    
            </table>
    </div>
</div>

