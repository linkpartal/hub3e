                <label style="font-size: 20px;font-weight: bold ;font:x-large Roboto;">
                    OPPORTUNITÉS PUBLIQUES

                </label>
            </div>
        </div>


        <table class="table table-bordered table-hover table-responsive col-lg-12">
            <thead style="background-color:#3988d7">
            <tr>
                <th style="text-align:center"> INTITULE</th>
                <th style="text-align:center"> SOCIETE</th>
                <th style="text-align:center"> DOMAINE</th>
                <th style="text-align:center"> DATE DEPOT</th>
                <th style="text-align:center"> ACTIONS</th>
            </tr>
            </thead>
            <tbody>
            {% if MissionsPublic is defined %}
                {% set i=0 %}
                {% for m in MissionsPublic %}
                    {% if EstRecupere(m.etablissement.id(),etablissement.getId()) == false %}
                        {% if i < 5 %}
                            <tr>
                                <td> {{ m.intitule }} </td>
                                <td> {{ m.etablissement.tier.raisonsoc }} </td>
                                <td> {{ m.domaine }} </td>
                                <td> {{ m.datecreation | date ('m/Y') }} </td>
                                <td>
                                    <button class="btn btn-default btn-xs ">
                                        <a href="{{ path('admin_afficheMissionpublic',{'id': m.getId,'idFor': "-1" } ) }}"
                                           target="_blank">
                                            <span class="glyphicon glyphicon-search" aria-hidden="true"></span>
                                        </a>
                                    </button>
                                </td>
                            </tr>
                        {% endif %}
                        {% set i=i+1 %}
                    {% endif %}

                {% endfor %}
                {# {% elseif missions == null %} #}
                {# <tr> Aucunes Missions à afficher </tr>
#}                {% endif %}
            </tbody>
        </table>

        <div style="text-align:center; align-items:center; margin-left:5px; width:96.3%; height:50px"
             class="col-xs-10 SASPaginate">
            <div class="col-xs-1" style="text-align: center; width: 100%">
                <button onclick="showDiv('MissionPublic')" class="btn btn-primary" type="button">
                    VOIR PLUS
                </button>
            </div>
        </div>
    </div>
</div>
